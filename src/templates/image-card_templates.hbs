{{#each this}}

<div class="photo-card image__item">
  <div class="thumb thumb-js">
  
  <img src={{webformatURL}} alt="" width="640" height="470" class="image.js" />
  
  </div>
  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      <span>{{likes}}</span>
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      <span>{{views}}</span>
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      <span>{{comments}}</span>
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      <span>{{downloads}}</span>
    </p>
  </div>
</div>
{{/each}}